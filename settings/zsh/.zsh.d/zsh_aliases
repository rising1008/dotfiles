# for docker
alias dcc="docker ps -aq -f status=exited -f status=dead | xargs docker rm"
alias dic="docker images -f "dangling=true" -q | xargs docker rmi"

# for navigation.
alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias .....="cd ../../../.."
alias ~="cd ~"

# for dev
alias iosls="xcrun simctl list devices"

# for shortcut.
alias v="/usr/local/bin/vim"
alias g="git"
alias h="history"
alias lock="m screensaver"
alias dnsflush='dscacheutil -flushcache'
